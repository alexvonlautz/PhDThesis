      \field{issn}{10959572}
      \field{journaltitle}{NeuroImage}
      \field{title}{{Neural representations of confidence emerge from the process of decision formation during perceptual choices}}
      \field{year}{2015}
      \verb{doi}
      \verb 10.1016/j.neuroimage.2014.11.036
      \endverb
      \keyw{Confidence,Decision making,EEG,Evidence accumulation,Linear discriminant analysis,Single-trial}
